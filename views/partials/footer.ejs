<script>

    let user = <%- JSON.stringify(user) %>;


    function acronym(sentence) {
        var matches = sentence.match(/\b(\w)/g);
        var acronym = matches.join('');

        return acronym;
    }

    if ($('.initials').text() != '')
        $('.initials').text(acronym($('.initials').text()));


</script>
